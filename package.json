{
  "name": "time-tracker-for-jira",
  "version": "0.0.9",
  "description": "Angular 6 with Electron (Typescript + SASS + Hot Reload)",
  "homepage": "https://github.com/tomreinartz90/jira-time-tracker",
  "author": {
    "name": "Tom Reinartz",
    "email": "tom.reinartz90@gmail.com"
  },
  "keywords": [
    "angular",
    "angular 6",
    "electron",
    "typescript",
    "sass",
    "timemanagement"
  ],
  "main": "main.js",
  "private": true,
  "scripts": {
    "serve": "npm-run-all -p ng:serve electron:serve",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "ng:serve": "ng serve",
    "ng:serve:web": "ng serve -o",
    "electron:serve-tsc": "tsc -p tsconfig-serve.json",
    "electron:serve": "wait-on http-get://localhost:4200/ && electron . --serve",
    "electron:local": "npm run build:prod && electron .",
    "electron:linux": "npm run build:prod && npx electron-builder build --linux",
    "electron:windows": "npm run build:prod && npx electron-builder build --windows",
    "electron:mac": "npm run build:prod && npx electron-builder build --mac",
    "publish": "npm run build:prod && npx electron-builder build --mac --win -p always",
    "publish:gitlab": "npm run build:prod && npx electron-builder build --mac --win"
  },
  "dependencies": {
    "@angular/animations": "^12.1.1",
    "@angular/cdk": "^12.1.1",
    "@angular/material": "^12.1.1",
    "color-hash": "^1.0.3",
    "electron-updater": "4.0.5",
    "lodash": "latest",
    "md5": "^2.2.1",
    "moment": "^2.24.0",
    "ms": "^2.1.1",
    "ng-packagr": "^12.1.2",
    "semver-bump": "^1.0.0",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.1.1",
    "@angular/cli": "12.1.1",
    "@angular/common": "12.1.1",
    "@angular/compiler": "12.1.1",
    "@angular/compiler-cli": "12.1.1",
    "@angular/core": "12.1.1",
    "@angular/forms": "12.1.1",
    "@angular/language-service": "12.1.1",
    "@angular/platform-browser": "12.1.1",
    "@angular/platform-browser-dynamic": "12.1.1",
    "@angular/router": "12.1.1",
    "@ngx-translate/core": "10.0.1",
    "@ngx-translate/http-loader": "3.0.1",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "2.0.3",
    "@types/node": "^12.11.1",
    "core-js": "2.5.6",
    "electron": "13.1.6",
    "electron-builder": "22.11.7",
    "electron-reload": "^1.5.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "npm-run-all": "4.1.3",
    "npx": "^10.2.2",
    "protractor": "~7.0.0",
    "rxjs": "6.6.7",
    "ts-node": "6.0.3",
    "tslint": "~6.1.0",
    "typescript": "4.3.5",
    "wait-on": "2.1.0",
    "webdriver-manager": "12.0.6",
    "zone.js": "~0.11.4"
  }
}
